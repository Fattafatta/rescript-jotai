{
  "name": "@fattafatta/rescript-jotai",
  "namespace": "jotai",
  "version": "0.3.1",
  "jsx": {
    "version": 4,
    "module": "react"
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": true,
      "public": [
        "Atom",
        "Provider",
        "Utils",
        "Store"
      ]
    },
    {
      "dir": "test",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": [
    {
      "module": "commonjs",
      "suffix": ".bs.js",
      "in-source": true
    }
  ],
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core"
  ],
  "bs-dev-dependencies": [
    "@greenlabs/rescript-jest",
    "@greenlabs/rescript-testing-library"
  ]
}
